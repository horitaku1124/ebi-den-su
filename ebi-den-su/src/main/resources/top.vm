<body>

<table>
    <tr>
        <th>TestTarget</th>
        <th>TestOverall</th>
        <!--
        <th>ClassName</th>
        <th>MethodName</th>
        -->
        <th>checkPoint</th>
        <th>Result</th>
    </tr>

    #foreach($row in $results)
